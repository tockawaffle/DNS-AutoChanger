{
  "name": "dns-autochanger",
  "version": "1.1.0",
  "description": "Just something I made because I didn't want to change my DNS settings manually",
  "main": "src/jobs/cron.ts",
  "scripts": {
    "main": "ts-node src/functions/test.ts && ts-node src/jobs/cron.ts",
    "main-nodemon": "nodemon --exec ts-node src/jobs/cron.ts",
    "linux": "pm2 start src/jobs/cron.ts", 
    "ncu": "ncu -u",
    "postinstall": "$(npm bin)/pm2 install typescript ts-node",
    "prestart": "ts-node src/functions/test.ts"
  },
