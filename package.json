{
  "name": "dns-autochanger",
  "version": "1.0.0",
  "description": "Just something I made because I didn't want to change my DNS settings manually",
  "main": "src/jobs/cron.ts",
  "scripts": {
    "main": "ts-node src/cron.ts",
    "main-nodemon": "nodemon --exec ts-node src/cron.ts",
    "linux": "pm2 start src/cron.ts",
    "ip": "ts-node src/functions/getIp.ts",
    "ncu": "ncu -u",
    "getdns": "ts-node src/functions/cloudflare/getIpDns.ts",
    "updatedns": "ts-node src/functions/cloudflare/client/update_dns_record.ts",
    "cron": "ts-node src/jobs/cron.ts",
    "postinstall": "$(yarn bin)/pm2 install typescript"
  },
  "keywords": [],
  "author": "Tockawa",
  "license": "MIT",
  "dependencies": {
    "@types/cron": "^2.0.0",
    "axios": "^1.1.3",
    "cron": "^2.1.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.3.15",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
