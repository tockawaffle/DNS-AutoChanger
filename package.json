  "license": "MIT",
  "dependencies": {
    "@types/cron": "^2.0.0",
    "axios": "^1.1.3",
    "cron": "^2.1.0",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "npm-check-updates": "^16.3.15",
    "pm2": "^5.2.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  }
}
